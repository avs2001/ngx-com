{
  "name": "ngx-com",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve integration",
    "build": "ng build",
    "build:lib": "ng build com --configuration production",
    "watch": "ng build --watch --configuration development",
